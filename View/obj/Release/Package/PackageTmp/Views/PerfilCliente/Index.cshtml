                            </div>
                        </div>

                        <!-- /.tab-pane -->
                        <div class="tab-pane" id="Direcciones">
                            <div class="table-responsive" style="overflow-y: hidden;">
                                <table id="gridDirecciones"></table>
                            </div>
                        </div>
                        
                        <!-- /.tab-pane -->
                        <div class="tab-pane" id="Empleos">
                            <div class="table-responsive" style="overflow-y: hidden;">
                                <table id="gridEmpleos"></table>
